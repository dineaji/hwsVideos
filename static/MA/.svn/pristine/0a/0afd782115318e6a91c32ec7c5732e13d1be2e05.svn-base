 .overlayEmail-bg .email_title h1 {font-family:YellowBalloon, Arial, Helvetica, sans-serif;font-size:30px;font-weight:bold;color:#FFF;}
 .overlayEmail-bg p {font-family:Arial, Helvetica, sans-serif;font-size:16px;color:#000;text-align:center;margin-left:90px;margin-top:47px;width:525px;}
 .overlayEmail-bg form {margin-left:210px;margin-top:10px;}
 .overlayEmail-bg .email_field {clear:both;margin-bottom:10px;}
 .overlayEmail-bg .email_txt {float:left;width:40px;line-height:27px;text-align:right;}
 .overlayEmail-bg span {font-family:Verdana, Geneva, sans-serif;font-weight:bold;font-size:12px;color:#040707;}
.input_box { border: 2px solid #00afef; height: 24px; width: 213px;margin-left:10px;color:#c5c5c6; -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;}
 .overlayEmail-bg .email_submit_btn {background:url("/CatalogContent/img/email_send.png") no-repeat; width:168px; height:44px;border:none;cursor:pointer;margin-left:48px;}
 .overlayEmail-bg .email_privacy {float:right;margin-right:50px;}
 .overlayEmail-bg .email_privacy a {color:#0087EF;font-family:Arial, Helvetica, sans-serif;font-size:12px;font-weight:bold;text-shadow:none;}
 .overlayEmail-bg .email_sent .header1 h1 {font-family:YellowBalloon, Arial, Helvetica, sans-serif;font-size:30px;font-weight:bold;color:#000;text-align:center;}
 .overlayEmail-bg .email_sent  p {font-family:Arial, Helvetica, sans-serif;font-size:16px;color:#000;text-align:center;margin-top:0;}
 .overlayEmail-bg .email_sent .post_img {text-align:center;}
 .overlayEmail-bg .email_sent .email_Navigation {clear:both;margin-left:210px;}
 .overlayEmail-bg .email_sent .email_Navigation ul {margin-left:-10px;}
 .overlayEmail-bg .email_sent .email_Navigation li{float:left;margin-top:16px;margin-left:2px;}
 .overlayEmail-bg .email_sent .email_Navigation img {cursor:pointer;}

