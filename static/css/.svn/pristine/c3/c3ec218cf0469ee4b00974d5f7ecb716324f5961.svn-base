/* header */
.navbar-default.hwr-header{min-height:100px;background-color:#0082c8;background-image:none;border-radius: 0px;-webkit-box-shadow:none;box-shadow:none;border:none;margin-bottom:0px;position:fixed;width:100%;max-width:1440px;z-index:9999}
.navbar-brand{padding:20px 30px}
.navbar-default.hwr-header .navbar-nav li{border-left:1px solid #0068a0;padding:0px 20px;min-width:170px}
.navbar-default.hwr-header .navbar-nav .top-nav-cars .subnav-bg  .bx-wrapper .bx-viewport li{min-width:220px}
/*.navbar-default .navbar-nav li:hover{background-color:#0068a0}*/
.navbar-default.hwr-header .navbar-nav > li > a{display:block;width:100%;height:100%}
/*Dinesh Changes for wishlist Nav Starts*/
.nav > li.wish-list {
  position: relative
}
.nav > li > a.shop-nav__counter {
  position: static;
  cursor: pointer;
  font-size: 1.5em;
}
.nav > li > a.shop-nav__counter.active {
  background: none !important
}
.navbar-default.hwr-header .navbar-nav .shop-nav__counter.active .number {
  opacity: 0;
  -webkit-transition opacity .5s linear;
  -moz-transition opacity .5s linear;
  transition opacity .5s linear
}
.nav > li > a.shop-nav__counter.active:after {
  animation: wishlistNav 1.5s steps(13);
  -webkit-transition: none;
  -moz-transition: none;
  transition: none;
  font-size: 21px
}
.nav > li > a.shop-nav__counter:after {
  content: "";
  background-image: url('../images/collections/nav_wishlist_activated.png') !important;
  background-size: 1300% auto;
  background-position: -1200% 0;
  width: 2.715em;
  height: 2.715em;
  margin: auto;
  top: 0;
  bottom: 0;
  position: absolute;
  left: 0;
  right: 0;
  font-size: 0;
  -webkit-transition: all 0.5s linear;
  -moz-transition: all 0.5s linear;
  transition: all 0.5s linear;
}
.navbar-default.hwr-header .navbar-nav .shop-nav__counter .number {
  position: absolute;
  left: 0;
  right: 0;
  text-align: center;
  font-size: 0.6em;
  top: 47%;
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  transform: translateY(-50%);
  color: #1a1a1a;
  opacity: 1;
  -webkit-transition opacity .5s linear;
  -moz-transition opacity .5s linear;
  transition opacity .5s linear;
  background: none;
  padding-left: 0.3em;
  font-family: "AgencyBlackItalic", sans-serif;
  display: inline-block;
  font-style:italic
}
@keyframes wishlistNav {
  from {
    background-position: 0px
  }
  to {
    background-position: -741px
  }
}
/*Dinesh Changes for wishlist Nav Ends*/
.navbar-default.hwr-header .navbar-nav > li:not(.wish-list) > a > span{background-image:url("../images/top-nav-icons.png");font-size:1.5em;font-family:"AgencyBlackItalic", sans-serif;line-height:70px;color:#fff;text-decoration:none;padding-left: 45px;background-repeat: no-repeat;font-style:italic;display:inline-block}
