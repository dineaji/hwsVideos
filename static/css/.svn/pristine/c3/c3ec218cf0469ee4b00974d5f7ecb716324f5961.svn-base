.primary,.secondary {pointer-events: none}
.primary{opacity: 1;transition: all 150ms linear;display:inline-block !important}
.secondary{position: absolute;top: 0;left: 0;opacity: 0;transform: translateX(-40%);transition: all 200ms linear}
.bxslider li .collections-hover:hover .primary{transform: translateX(35%);opacity: 0}
.bxslider li .collections-hover:hover .secondary{transform: translateX(0);opacity: 1;transition: all 200ms ease-out}
.collections-hover{position:relative}
.top-nav-cars:hover .bx-wrapper li .collections-hover .collection-title-bg{height: 40px;transform: skewX(-15deg);bottom:0px;position:absolute;width:125%;margin-left:0px;opacity:0;left:-6px}
.top-nav-cars:hover .bx-wrapper li:hover .collections-hover .collection-title-bg{margin-left:0px;transition: margin 0.5s;-moz-transition: margin 0.5s; 
    -webkit-transition: margin 0.5s; -o-transition: margin 0.5s;opacity:1}
.top-nav-cars:hover .bx-wrapper li .collections-hover .collection-title{line-height: 1.5em;position: absolute;bottom: 8px;text-align: center;width: 100%;font-family:"AgencyFBBoldItalic", sans-serif;font-weight:bold;text-decoration:none;color:#fff;font-size:1.5em;transform:skewX(5deg);font-style:italic}
#test123:before {background-color: #d9232e; }

.collection-title:before {
  background-color: #ed3422;
  position: absolute;
  color: transparent;
  z-index: -1;
  padding: 0.2em 0.8em;
  left: 50%;
  top: 50%;
  opacity: 0;
  transform: translate(-100%, -56%) skewX(-19deg);
  transition: all 200ms ease-in;
  white-space: nowrap;
  display: inline-block;
}

li:hover .collection-title:before {
  transform: translate(-50%, -56%) skewX(-19deg);
  opacity: 1;
  transition: all 200ms ease-out;
}

