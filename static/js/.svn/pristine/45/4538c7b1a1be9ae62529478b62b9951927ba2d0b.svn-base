                $('.loader').hide();
                $('#mailSendBtn').removeAttr('disabled', 'disabled');
            }
        }, this));
    },

    // gets fields from form and converts to JSON output
    prepareFormData: function($fields){
        var o = {};
        var a = $fields.serializeArray();
        $.each(a, function() {
